{
  "hosting": [
    {
      "target": "production",
      "public": "dist",
      "ignore": [
        "**/node_modules/**",
        "**/.*",
        "craco.config.ts",
        "firebase.json",
        "package.json",
        "README.md",
        "tsconfig.extend.json",
        "tsconfig.json",
        "yarn.lock"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "development",
      "public": "dist",
      "ignore": [
        "**/node_modules/**",
        "**/.*",
        "craco.config.ts",
        "firebase.json",
        "package.json",
        "README.md",
        "tsconfig.extend.json",
        "tsconfig.json",
        "yarn.lock"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}